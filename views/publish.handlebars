{{>dashboard}}
 <main>
      <div class="py-6">
        <div class="mx-auto max-w-7xl px-4 sm:px-6 md:px-8">
          <h1 class="text-2xl font-semibold text-gray-900">Publish</h1>
        </div>
        <div class="mx-auto max-w-8xl px-4 sm:px-6 md:px-8">
          <!-- Replace with your content -->
          <div class="py-12 sm:py-16 lg:py-20">
    <div class="px-4 mx-auto max-w-7xl sm:px-6 lg:px-8">
        <div class="sm:flex sm:items-center sm:justify-between">

            <div class="flex items-center justify-start mt-4 sm:justify-end sm:mt-0 sm:space-x-7">
                <button type="button" class="items-center hidden px-3 py-2 text-sm font-medium leading-4 text-gray-700 bg-white border border-gray-300 rounded-lg shadow-sm sm:inline-flex hover:bg-gray-50 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-gray-500">
                    <svg class="w-4 h-4 mr-1 -ml-1" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="2">
                        <path stroke-linecap="round" stroke-linejoin="round" d="M12 10v6m0 0l-3-3m3 3l3-3m2 8H7a2 2 0 01-2-2V5a2 2 0 012-2h5.586a1 1 0 01.707.293l5.414 5.414a1 1 0 01.293.707V19a2 2 0 01-2 2z" />
                    </svg>
                    Approve All
                </button>

                <div class="inline-flex items-center justify-end">
                    <label for="sort" class="text-base font-medium text-gray-900 sm:text-sm"> Filter: </label>
                    <select id="sort" name="sort" class="block w-full py-2 pl-1 pr-10 text-base border-gray-300 border-none rounded-lg focus:outline-none focus:ring-indigo-600 focus:border-indigo-600 sm:text-sm">
                        <option>Pending</option>
                        <option>Approved</option>
                        <option>Declined</option>
                    </select>
                </div>
            </div>
        </div>

        <div class="flex flex-col mt-4 lg:mt-8">
            <div class="-mx-4 -my-2 overflow-x-auto sm:-mx-6 lg:-mx-8">
                <div class="inline-block min-w-full py-2 align-middle md:px-6 lg:px-8">
                    <table class="min-w-full lg:divide-gray-200 lg:divide-y ">
                        <thead class="hidden lg:table-header-group">
                            <tr>
                              <th class="py-3.5 px-4 text-left text-xs font-medium text-gray-500 uppercase tracking-widest"><input type="checkbox"></th>
                                <th class="py-3.5 px-4 text-left text-xs font-medium text-gray-500 uppercase tracking-widest">Content</th>

                                <th class="py-3.5 px-4 text-left text-xs uppercase tracking-widest font-medium text-gray-500">Media</th>

                                <th class="py-3.5 px-4 text-left text-xs uppercase tracking-widest font-medium text-gray-500">Date Created</th>

                                <th class="py-3.5 px-4 text-left hidden xl:table-cell text-xs uppercase tracking-widest font-medium text-gray-500">Created By</th>

                                <th class="relative py-3.5 pl-4 pr-4 md:pr-0">
                                    <span class="sr-only"> Actions </span>
                                </th>
                            </tr>
                        </thead>

                        <tbody>
                          {{>post}}
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

              <!-- /End replace -->
        </div>
      </div>
    </main>